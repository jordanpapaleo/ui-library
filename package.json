    "@storybook/react": "3.3.13",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-react-css-modules": "3.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
