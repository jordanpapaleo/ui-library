{
  "name": "@jordanpapaleo/ui-library",
  "version": "1.0.7",
  "description": "UI Library",
  "keywords": [
    "react",
    "storybook"
  ],
  "main": "dist/ui-library.js",
  "author": "jordanpapaleo",
  "license": "MIT",
  "homepage": "https://github.com/jordanpapaleo/ui-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:jordanpapaleo/ui-library.git"
  },
  "bugs": {
    "url": "https://github.com/jordanpapaleo/ui-library/issues"
  },
  "scripts": {
    "start": "start-storybook -p 4040 -c .storybook",
    "pretest": "npm run test:lint",
    "build": "sh build.sh",
    "test": "npm run test:unit",
    "test:lint": "./node_modules/.bin/eslint src/",
    "test:unit": "./node_modules/.bin/jest ."
  },
  "dependencies": {
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.13",
    "@storybook/addon-knobs": "3.3.13",
    "@storybook/addon-notes": "3.3.13",
    "@storybook/addon-options": "3.3.13",
    "@storybook/react": "3.3.13",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-react-css-modules": "3.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-app": "3.0.1",
    "babel-runtime": "6.23.0",
    "chance": "1.0.13",
    "eslint": "4.1.1",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "jest": "22.3.0",
    "standard": "10.0.2",
    "webpack": "3.11.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
